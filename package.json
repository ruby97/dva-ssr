{
  "private": true,
  "scripts": {
    "start": "roadhog server",
    "build": "roadhog build",
    "lint": "eslint --ext .js src test",
    "precommit": "npm run lint",
    "buildClient": "webpack --config webpack.config.client.js",
    "buildServer": "cross-env BABEL_ENV=server webpack --config webpack.config.server.js",
    "ssr": "nodemon dist/server.js",
    "format": "prettier-eslint \"src/**/*.js\"",
    "route": "node tool/generator.js && prettier-eslint \"src/routes.js\"",
    "dev": "npm run route && npm run buildClient && npm run buildServer && npm run ssr",
    "devssr": "npm run buildServer && npm run ssr"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "react-loadable/babel"
    ]
  },
  "dependencies": {
    "antd": "^3.9.3",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "dva": "^2.4.0",
    "hapi": "^17.6.0",
    "history": "latest",
    "inert": "^5.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "less-loader": "^4.1.0",
    "prop-types": "latest",
    "react": "^16.2.0",
    "react-css-modules": "^4.7.7",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.5.0",
    "react-router-config": "^4.4.0-beta.1",
    "react-router-dom": "latest",
    "react-router-redux": "^4.0.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-dva-hmr": "^0.3.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^1.0.0",
    "eslint": "^4.14.0",
    "eslint-config-umi": "^0.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.12.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.12.5",
    "prettier-eslint-cli": "^4.7.1",
    "redbox-react": "^1.4.3",
    "roadhog": "^2.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.0",
